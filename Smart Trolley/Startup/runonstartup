#!/bin/bash
sleep 5  # Add a small delay to ensure the desktop environment has fully loaded
python3 /path/to/your/pyqt5_app.py  # Replace with the correct path to your PyQt5 application's script

chmod +x /path/to/your/start_my_app.sh

nano ~/.config/lxsession/LXDE-pi/autostart

@/path/to/your/start_my_app.sh

sudo reboot



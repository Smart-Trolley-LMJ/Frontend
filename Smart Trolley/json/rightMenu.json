{
	"QCustomSlideMenu": [{
		"name": "RightMenuContainer",
		"defaultSize": [{
			"width": 0,
			"height": "auto"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "auto"
		}],
		"expandedSize": [{
			"width": 200,
			"height": "auto"
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		}],
		"toggleButton": [{
			"buttonName": "toggleAddbtn",
			"icons": [{
				"whenMenuIsCollapsed": "./UI/Images/White Icons/plus-square.svg",
				"whenMenuIsExpanded": "./UI/Images/White Icons/x.svg"
			}]
		}]
	}]
} 